
public class Test {
	public static void main(String[] args) throws Exception {
		Integer[][] list = { { 2, 1 }, { 3, 2 }, { 4, 3 }, { 5, 4 }, { 7, 6 }, { 14, 13 }, { 15, 14 } };
		int u = 75;

		int k = 20;
		System.out.println("k = " + k + ", highsqrt:" + highSqRt(k) + "* lowsqrt:" + lowSqRt(k) + " = "
				+ highSqRt(k) * lowSqRt(k));

		vEBtreePriorityQueue vebqueue = new vEBtreePriorityQueue(u, list);

		for (int i = 0; i < list.length; i++) {
			System.out.println(vebqueue.ExtractMax());
		}

	}

	private static Double highSqRt(Integer x) {
		return Math.pow(2, Math.ceil(log2(x) / 2));
	}

	private static Double lowSqRt(Integer x) {
		return Math.pow(2, Math.floor(log2(x) / 2));
	}

	private static Double log2(Integer x) {
		return (Math.log10(x) / Math.log10(2));
	}
}
